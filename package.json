{
  "name": "jcc2d",
  "version": "1.0.1",
  "description": "a light weight canvas 2d render & an awesome animator",
  "main": "build/jcc2d.min.js",
  "directories": {
    "example": "examples"
  },
  "files": [
    "changelog.md",
    "README.md",
    "build",
    "src"
  ],
  "scripts": {
    "release": "npm run build && gulp release",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "preversion": "npm run release && git add -A && git commit -m 'release: release new version v%s'",
    "patch": "npm version patch -m 'release: v%s'",
    "postversion": "git push github-origin master && git push github-origin v$npm_package_version",
    "docs": "jsdoc -c scripts/jsdoc.conf.json -R README.md",
    "test": "karma start",
    "lint": "eslint .",
    "fix": "eslint --fix ./src",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg",
    "changelog": "git changelog"
  },
  "keywords": [
    "canvas",
    "canvas 2d",
    "renderer",
    "browser",
    "animation",
    "canvas engine",
    "javascript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jasonChen1982/jcc2d.git"
  },
  "author": {
    "name": "jason Chen",
    "email": "jasonchen1882@gmail.com",
    "url": "http://jason82.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.0",
    "chai": "^3.2.0",
    "eslint": "^3.8.1",
    "eslint-config-google": "^0.7.1",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^0.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "gulp-uglify": "^1.5.3",
    "husky": "^0.13.2",
    "jaguarjs-jsdoc": "^1.0.1",
    "jsdoc": "^3.4.2",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^3.1.0",
    "rollup": "^0.36.3",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-watch": "^2.5.0",
    "validate-commit-msg": "^2.11.1"
  },
  "dependencies": {},
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "release",
        "build",
        "revert",
        "deps"
      ]
    }
  }
}
